#include<iostream>
using namespace std;
int main() {

	int age;

	cout << "enter your cookies:" << endl;
	cin >> age;

	if (age < 5)
		cout << "that's not enough cookies!";

	else if (age < 10)
		cout << "that's a reasonable number of cookies!";

	else  (age < 15)
		cout << "that's a lot of cookies, can I have one!";

	else if (age <= 15)
	cout
	<< "SUMMON THE COOKIE MONSTER OM NOM NOM";



}
